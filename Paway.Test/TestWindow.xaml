<WindowEXT x:Class="Paway.Test.TestWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Paway.Test"
        mc:Ignorable="d"
        UseLayoutRounding="True"
        DataContext="{Binding Test ,Source={StaticResource Locator}}"
        Title="TestWindow" Height="450" Width="800" WindowStartupLocation="CenterScreen">
    <Grid>
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center" Width="74">
            <CheckBoxEXT Content="123"/>
            <CheckButton Content="123" Type="Error"/>
            <ButtonEXT Content="123"/>
            <ProgressBarEXT Value="50" Orientation="Vertical"/>
            <ProgressRound Value="35" />
            <ProgressBoard Value="35" />

        </StackPanel>
    </Grid>
</WindowEXT>
