<Window x:Class="Paway.Test.PlotWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Paway.Test"
        mc:Ignorable="d"
        DataContext="{Binding Plot ,Source={StaticResource Locator}}"
        xmlns:oxy="http://oxyplot.org/wpf"
        Title="PlotWindow" Height="450" Width="900" WindowStartupLocation="CenterScreen">
    <StackPanel>
        <Border BorderThickness="1" BorderBrush="LightGray">
            <oxy:PlotView Model="{Binding Path= PlotModel}" Height="250" MinWidth="880">
            </oxy:PlotView>
        </Border>
    </StackPanel>
</Window>
