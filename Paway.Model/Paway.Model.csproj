<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<UseWPF>true</UseWPF>
		<TargetFrameworks>net452</TargetFrameworks>
		<!--用于指示编译器是否生成文档文件的值-->
		<GenerateDocumentationFile>True</GenerateDocumentationFile>
		<!--不自动生成程序集信息(版本号)-->
		<GenerateAssemblyInfo>false</GenerateAssemblyInfo>
		<Tags>WPF Model</Tags>
		<AssemblyName>Paway.Model</AssemblyName>
		<RootNamespace>Paway.Model</RootNamespace>
		<OutputPath>..\bin\$(Configuration)</OutputPath>
	</PropertyGroup>

	<ItemGroup>
		<Compile Include="..\AssemblyVersion.cs">
			<Link>Properties\AssemblyVersion.cs</Link>
		</Compile>
	</ItemGroup>
	<ItemGroup>
		<Resource Include="Images\*\*.png" />
		<EmbeddedResource Include="Office\license.elic.xml" />
	</ItemGroup>

	<ItemGroup>
		<Reference Include="Microsoft.CSharp" />
		<Reference Include="System.Windows.Forms" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\Paway.WPF\Paway.WPF.csproj" />
	</ItemGroup>
	<ItemGroup>
		<PackageReference Include="Paway.Helper" Version="2.5.3" />
		<PackageReference Include="Paway.Utils" Version="2.5.3" />
		<PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
		<PackageReference Include="OxyPlot.Core" Version="2.1.0" />
		<PackageReference Include="MvvmLightLibs" Version="5.4.1.1" />
		<PackageReference Include="Spire.Office" Version="8.11.0" />
	</ItemGroup>

</Project>