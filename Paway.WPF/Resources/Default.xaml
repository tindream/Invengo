    <Style x:Key="TextLeft" TargetType="{x:Type TextBlock}" >
        <Setter Property="Margin" Value="2,0" />
        <Setter Property="HorizontalAlignment" Value="Left" />
        <Setter Property="VerticalAlignment" Value="Center" />
        <Setter Property="TextTrimming" Value="WordEllipsis"/>
    </Style>
    <Style x:Key="TextCenter" TargetType="{x:Type TextBlock}">
        <Setter Property="Margin" Value="2,0" />
        <Setter Property="HorizontalAlignment" Value="Center" />
        <Setter Property="VerticalAlignment" Value="Center" />
        <Setter Property="TextTrimming" Value="WordEllipsis"/>
    </Style>
    <Style x:Key="TextRight" TargetType="{x:Type TextBlock}">
        <Setter Property="Margin" Value="2,0" />
        <Setter Property="HorizontalAlignment" Value="Right" />
        <Setter Property="VerticalAlignment" Value="Center" />
        <Setter Property="TextTrimming" Value="WordEllipsis"/>
    </Style>

    <Style x:Key="themeBackground" TargetType="Control">
        <Setter Property="Background" Value="{Binding Path=(local:ThemeEXT.Background).Value, RelativeSource={RelativeSource Self}}" />
    </Style>
    <Style x:Key="themeBlockHigh" TargetType="TextBlock">
        <Setter Property="Background" Value="{Binding Path=(local:ThemeEXT.HighBrush).Pressed, RelativeSource={RelativeSource Self}}" />
    </Style>
    <Style x:Key="themeBackHigh" TargetType="Control">
        <Setter Property="Background" Value="{Binding Path=(local:ThemeEXT.HighBrush).Pressed, RelativeSource={RelativeSource Self}}" />
    </Style>
    <Style x:Key="themeFillHigh" TargetType="Shape">
        <Setter Property="Fill" Value="{Binding Path=(local:ThemeEXT.HighBrush).Pressed, RelativeSource={RelativeSource Self}}" />
    </Style>
    <LinearGradientBrush x:Key="ColorSelector">
